pipeline {
    agent { label 'slave2' }
    stages {
        stage('Build') {
            steps {
                git branch: 'master', url: 'https://github.com/jenkins-docs/simple-java-maven-app.git'
                sh '''
                    #!/bin/bash 
                    pwd 
                    ls > log-buildfile
                    echo "This is build stage"
                    mvn clean package
                ''' 
            }
        }
    }
}
